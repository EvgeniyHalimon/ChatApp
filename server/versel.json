{
  "version": 2,
  "builds": [
    {
      "src": "build-command",
      "use": "@vercel/static"
    }
  ],
  "output": {
    "root": "public"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./server.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}